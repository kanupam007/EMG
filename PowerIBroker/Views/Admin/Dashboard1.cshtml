@{
    ViewBag.Title = "Dashboard";
   Layout = "~/Views/Shared/_emgadminlayout.cshtml";
}
@{
    List<PowerIBrokerDataLayer.ContactUsEnquiry> enquiries = ViewBag.Enquiries;
}
<div id="page-wrapper">

    <div class="container-fluid">
        <div>

        </div>
        <div class="col-lg-12">

            <div class="row">
                <div class="col-sm-12">
                   
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h3 class="panel-title">Enquiries</h3>
                        </div>
                        <div class="panel-body">
                           @if (enquiries.Count > 0)
{
    
        <div class="table-responsive">
            <table class="table table-bordered table-hover table-striped">
                <thead>
                    <tr>

                        <th width="20%">Name</th>
                        <th width="10%">Email</th>
                        <th width="10%">Phone</th>
                        <th width="40%">Message</th>
                        <th width="20%">Date</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var content in enquiries)
                    {
                        <tr>
                            <td>
                                <span>@content.Name</span>
                            </td>

                            <td>
                                <span>@content.Email</span>
                            </td>
                            <td>
                                <span>@content.Phone</span>
                            </td>
                            <td>
                                <span>@content.Message</span>
                            </td>
                            <td>
                                <span>@String.Format("{0:MMM dd, yyyy}", content.AddedOn)</span>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
            <a href="@Url.Content("~/Admin/ManageContactUsEnquiries")" class="btn btn-xs btn-primary">View All</a>
        </div>
    
}
else
{
    <span>No new Enquries found</span>
}
                        </div>
                    </div>
                </div>
             
            </div>
            

            
        </div>


    </div>
</div>




@model PowerIBrokerDataLayer.AgeRangeLife
@{
    ViewBag.Title = "Imputed Income";
    Layout = "~/Views/Shared/_emgadminlayout.cshtml";
}

<link href="~/AdminContent/plugins/datatables/jquery.dataTables.css" rel="stylesheet" />
<script src="~/AdminContent/plugins/datatables/jquery.dataTables.min.js"></script>
<script src="~/Content/Frontend/js/jquery.mask.js"></script>
<script src="~/AdminContent/customjs/admincustom.js"></script>

<section class="content-header">
    <h1> Imputed Income</h1>

    <ol class="breadcrumb">
        <li><a href="/Admin/Dashboard"><i class="fa fa-dashboard"></i>Dashboard</a></li>
        <li class="active">Imputed Income</li>
    </ol>
</section>


<!-- Main content -->
<section class="content">

    <!-- Info boxes -->
    <div class="row">
        <div class="col-md-12">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title">Imputed Income</h3>

                </div><!-- /.box-header -->
                <div class="col-lg-12" style="padding-right:0px;padding-left:0px">
                    <div class="@(TempData["CustomMessage"] != null ? TempData["MessageType"] == "Success" ? "alert alert-success" : "alert alert-danger" : "")">
                        @if (TempData["CustomMessage"] != null)
                        {
                            <i class="fa @(TempData["MessageType"] == "Success" ? "fa fa-check" : "fa fa-times-circle")"></i>

                        }
                        <span>@TempData["CustomMessage"]</span>
                    </div>
                </div>
                @using (Html.BeginForm("ImputedIncome", "Admin", FormMethod.Post, new { Id = "formIdImputedIncome" }))
                {
                    @Html.HiddenFor(model => model.ID)
                    <div class="box-body">

                        <form role="form">
                            <div class="box-body">
                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label for="exampleInputEmail1">Start Age</label>
                                        @Html.TextBoxFor(model => model.AgeStart, new { @class = "form-control", placeholder = "Start Age", id = "AgeStart", maxlength = "3", onkeypress = "return isNumber(event)" })
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label for="exampleInputEmail1">End Age</label>
                                        @Html.TextBoxFor(model => model.AgeEnd, new { @class = "form-control", placeholder = "End Age", id = "AgeEnd", maxlength = "3", onkeypress = "return isNumber(event)" })
                                    </div>
                                </div>
                                <div class="col-lg-3">
                                    <div class="form-group">
                                        <label for="exampleInputEmail1">Monthly Rate Per Thousand</label>
                                        @Html.TextBoxFor(model => model.EmployeeValue, "{0:n2}", new { @class = "form-control numericTextBox", placeholder = "Monthly Rate Per Thousand", id = "EmployeeValue", maxlength = "8" })
                                    </div>
                                </div>


                            </div><!-- /.box-body -->

                            <div class="col-lg-3">
                                @*<div class="checkbox">
                                        @if (Model.ID == 0)
                                        {
                                            @Html.CheckBox("Status", new { @class = "checkBx", @checked = "checked" })
                                        }
                                        else
                                        {
                                            @Html.CheckBox("Status", new { @class = "checkBx" })
                                        }

                                        <label style="margin-left:0px">Is Active?</label>
                                    </div>*@

                                <div class="box-footer">
                                    <button type="button" id="btnSaveImputedIncome" class="btn btn-primary">Submit</button>
                                </div>
                            </div>

                        </form>

                        <br /><br /><br />
                        <!-- Main content -->
                        <section class="content">
                            <!-- Info boxes -->

                            <div class="row">
                                <div class="col-md-12">


                                    <div class="box">
                                        <div class="box-header">

                                            <h3 class="box-title">Imputed Income - List</h3>
                                            @*<input type="checkbox" id="value" onclick="GetDataImputedIncome()" style="float:right;" />
                                                <label style="float:right;margin-right:10px">Show Inactive Records</label>*@

                                        </div><!-- /.box-header -->
                                        <div class="box-body">

                                            @if (ViewBag.ImputedIncome.Count > 0)
                                            {
                                                @Html.Partial("_ImputedIncome");
                                            }
                                            else
                                            {
                                                <span style="margin-left: 226px; font-size: 15px;">No record found</span>
                                            }
                                        </div><!-- /.box-body -->
                                    </div>
                                    <!-- /.box-footer -->

                                </div><!-- /.col -->
                            </div>
                        </section><!-- /.content -->
                    </div><!-- ./box-body -->
                }
                <!-- /.box-footer -->
            </div><!-- /.box -->
        </div><!-- /.col -->
    </div>
</section>
<!-- /.content -->

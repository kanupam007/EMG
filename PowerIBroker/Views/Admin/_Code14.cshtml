<table id="example1" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th width="20%">Code(14) Name</th>
            <th width="25%">Name</th>
            <th width="25%">Description</th>
            <th width="10%">Status</th>
            <th width="20%">Action</th>
        </tr>
    </thead>
    <tbody>
        @if (ViewBag.Code14 != null)
        {

            foreach (var plancat in ViewBag.Code14)
            {
        <tr>
            <td>@plancat.CodeName</td>
            <td>@plancat.Name</td>
            <td>@plancat.Description</td>
            <td>
                @(plancat.IsActive == true ? "Active" : "Inactive")

            </td>
            <td>
                @if (plancat.IsDeleted != true)
                {
                    <a href="@Url.Action("EditCode14", "Admin", new { @id = plancat.ID })" class="btn btn-primary btn-xs">Edit</a>
                    @*<a class="btn btn-danger btn-xs">De-Activate</a>
                <a class="btn btn-success btn-xs">Activate</a>*@
                    <a data-exclude="true" href="javascript:void(0)" onclick="return DeleteCode14(@plancat.ID);" class="btn btn-primary btn-xs" style="background-color:red;border-color:red"> Delete</a>
                }
            </td>
        </tr>
            }
        }



    </tbody>
</table>
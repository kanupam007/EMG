<table id="tblSystemConfigAdmin" class="table table-bordered table-striped">
    <thead>
        <tr>
            <td width="35%" style="font:bold;">Key Name</td>         
            <td width="15%" style="font:bold;">Status</td>
            <th width="20%">Action</th>
        </tr>
    </thead>
    <tbody>
        @if (ViewBag.NotificationAdmin != null)
        {
            foreach (var notificationConfigurationAdmin in ViewBag.NotificationAdmin)
            {
                <tr>
                    <td>@notificationConfigurationAdmin.PropertyName</td>               
                    <td>
                        @(notificationConfigurationAdmin.IsActive == true ? "Active" : "In-Active")
                    </td>
                    <td>
                        <a href="@Url.Action("EditNotification", "Admin", new { @id = notificationConfigurationAdmin.Id })" class="btn btn-primary btn-xs">Edit</a>
                    </td>
                </tr>
            }
        }

    </tbody>
</table>
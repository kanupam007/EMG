@{
    Layout = null;
}

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js"></script>

<script type="text/javascript">
    $(function () {
        $("#pdf").bind("contextmenu", function (e) {
            e.preventDefault();
        });
    });
</script>



<style>
    @@media print{
body {display:none;}
}

    #pdf {
        position: relative;
        width:50%;
    }
        #pdf:after {
            position: absolute;
            content: '';
            width: calc(100% - 15px);
            left: 0;
            top: 0;
            bottom: 0;
            right: 15px;
            z-index: 999;
        }

</style>

<div>   
    <h1>Welcome to PDF Page</h1>
    <div id="pdf">
        <object width="100%" height="500px"   type="application/pdf" data="~/Content/Demo.pdf#embedded=true&toolbar=0&navpanes=0" id="pdf_content"></object>
    </div>

</div>

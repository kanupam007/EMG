    <link href="~/Content/sweetalert-master/dist/sweetalert.css" rel="stylesheet" />
    <script src="~/Content/sweetalert-master/dist/sweetalert-dev.js"></script>
    <script src="~/Content/sweetalert-master/dist/sweetalert.min.js"></script>
    <script src="https://www.google.com/recaptcha/api.js" async defer></script>
</head>
<body>
    <div class="mainCon">
        <!--header-->
        <div class="headerCon">
            <div class="logo">
                <a href="@Url.Content("~/")">
                    <img src="@Url.Content("~/content/frontend/images/landing-logo.png")">
                </a>
            </div>
            <!--for resonsive menu-->

            <div class="MenuBack"></div>
            <button data-page-id="home" class="btn-menu normal">
                <span>Menu</span>
            </button>
            <!--for resonsive menu-->
            <div class="navCon">
                <img src="@Url.Content("~/content/frontend/images/landing-logo.png")" class="reslogo">
                <ul>
                    @*<li class="navActive"><div class="navItem"><a href="#benifts" class="scroll">BENEFITS</a><div class="backOver"></div></div><div class="bdrForeResponsive"></div></li>*@
                    <li><div class="navItem"><a href="~/#feature">FEATURES</a><div class="backOver"></div></div><div class="bdrForeResponsive"></div></li>
                    @*<li><div class="navItem"><a href="#pricing" class="scroll">PRICING</a><div class="backOver"></div></div><div class="bdrForeResponsive"></div></li>*@
                    @*<li><div class="navItem"><a href="#reqDemo" class="scroll">Request Demo</a><div class="backOver"></div></div><div class="bdrForeResponsive"></div></li>*@
                    <li><div class="navItem"><a  href="@Url.Action("Login", "Home")" class="login">Login</a><div class="backOver"></div></div><div class="bdrForeResponsive"></div></li>
                    @*<li><a href="@Url.Action("Index", "UserLogin")">Login</a></li>*@

                    <li><div class="navItem"><a id="signUpLink" href="~/#signUp">Schedule Demo</a><div class="backOver"></div></div></li>
                </ul>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>

        </div>
        <!--header-->
        @RenderBody()

        <!--topFooter-->
        <div class="topFooter">
            <div class="container">
                <div class="foterNav">
                    <h2>Overview</h2>
                    @*<a href="#benifts" class="scroll">Benefits</a>*@
                    <a href="~/#feature">Features</a>
                    @*<a href="#pricing" class="scroll">Pricing</a>*@
                    <a href="@Url.Content("~/AboutUs")" class="">About Us</a>
                    <a href="@Url.Content("~/FAQs")" class="">FAQ’s</a>
                    <a href="@Url.Content("~/Contact")" class="">Contact Us</a>
                    <a href="@Url.Content("~/Security")" class="">Security</a>
                    <a href="@Url.Content("~/Terms-Conditions")" class="">Terms of Use</a>
                    <a href="@Url.Content("~/Privacy-Policy")" class="">Privacy Policy</a>
                </div>

                <script src="~/Content/Frontend/js/helium.mutate.js"></script>
                <script src="~/Content/Frontend/js/helium.slider.js"></script>
                <link href="~/Content/Frontend/css/heliumSlider.css" rel="stylesheet" />
                <div class="footerNews">
                    <div id="newsContent" class="helium-slider loading">


                        <div class="slide-window">
                            <ul class="slide-holder">
                                @{
                                    var latestNewsContent = obj.GetNewsContent(false);
                                    if (latestNewsContent.Count > 0)
                                    {
                                        foreach (var news in latestNewsContent)
                                        {
                                            <li>
                                                <div class="pane">
                                                    <h2>@news.Title</h2>
                                                    <p>@news.Description</p>
                                                    <a target="_blank" class="newsLink" href="@news.Link">@news.Link</a>
                                                </div>
                                            </li>
                                        }
                                    }
                                }


                            </ul>
                        </div>
                        <ul class="slide-nav"></ul>
                    </div>
                </div>

                @*<div class="footContactus" id="formEnquiry" style="width:26%">
                    <img src="~/Content/Frontend/images/ISI.png" width="300px" height="150px" />
                    <p style="color: black; font-size:12px;text-align:justify">Enroll My Group support our troops, police and fire departments</p>
                </div>
                <div class="clearfix">
                </div>*@

            </div>
        </div>
        <!--topFooter-->
        <!--footer-->
        <div class="footerCon">
            <div class="container">
                <div class="copyRight"> Copyright &copy; @DateTime.Now.Year</div>
                <div class="socIcons">

                    @{
                        var socialMediaLinks = obj.GetSocialMediaLink(false);
                    }
                    @if (socialMediaLinks.Count > 0)
                    {
                        foreach (var link in socialMediaLinks)
                        {
                            <a href="@link.Link" class="@(link.Title.ToLower() == "facebook" ? "facebook" : link.Title.ToLower() == "twitter" ? "tw" : link.Title.ToLower() == "linkedin" ? "in" : "tube")"></a>
                        }
                    }

                </div>
                <div class="clearfix"></div>

            </div>
        </div>
        <!--footer-->
        <!--LoginPop -->
        <div class="backbg"></div>
        @*@using (Html.BeginForm("ValidateCaptcha", "UserLogin"))*@
        @*@using (Html.BeginForm("Index", "UserLogin", FormMethod.Post, new { id = "Loginformsubmit" }))

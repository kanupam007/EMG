<!DOCTYPE html>
@using PowerIBrokerBusinessLayer;
@{
    CmsManagement obj = new CmsManagement();
    ViewBag.ContactUsContent = obj.GetContactUsContent();
    PowerIBrokerDataLayer.ContactUsPageContent ContactUsContent = ViewBag.ContactUsContent;

    Administration Admin = new Administration();
    ViewBag.IsCpatchaEmployee = Admin.IsCpatcha("CaptchaOnEmployeeLogin");

}
<html>
<head>
    <link rel="shortcut icon" type="image/ico" href="~/Content/Frontend/images/favicon.ico">
    <meta charset="utf-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <title>@(ViewBag.MetaTitle == "" || ViewBag.MetaTitle == null ? "EnrollMyGroup" : ViewBag.MetaTitle)</title>
    @if (!string.IsNullOrEmpty(ViewBag.MetaDescription))
    {
        <meta name="description" content="@ViewBag.MetaDescription">
    }
    @if (!string.IsNullOrEmpty(ViewBag.MetaKeywords))
    {
        <meta name="keywords" content="@ViewBag.MetaKeywords">
    }
    @if (!string.IsNullOrEmpty(ViewBag.MetaRobots))
    {
        <meta name="robots" content="@ViewBag.MetaRobots">
    }


    <!--css-->
    <link rel="stylesheet" href="@Url.Content("~/content/frontend/css/fonts.css")" media="all">
    <link rel="stylesheet" href="@Url.Content("~/content/frontend/css/style.css")" media="all">
    <link rel="stylesheet" href="@Url.Content("~/content/frontend/css/animate.css")" media="all">
    <link rel="stylesheet" href="@Url.Content("~/content/frontend/css/responsive.css")" media="all">
    <!--css-->
    @*<title>ENROLLMYGROUP</title>*@
    <script type="text/javascript" src="@Url.Content("~/content/frontend/js/jquery.min.js")"></script>
    <script src="@Url.Content("~/Content/Frontend/js/validations.js")"></script>
    <link href="~/Content/Frontend/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Content/Frontend/js/jquery.min.js"></script>
    <script src="~/Content/Frontend/js/toastr.min.js"></script>
    <link href="~/Content/Frontend/css/toastr.min.css" rel="stylesheet" />
    <script src="~/Content/Frontend/js/bootstrap.min.js"></script>

                        </li>
                        
                        <li class="forgotPass"><input style="margin: 1px 0 0;" type="checkbox" name="rememberMe" value="true" id="rememberMe" /><strong>&nbsp; Remember me</strong><a class="showPopup forgpass" href="#forgotPassPopup" data-toggle="modal">Forgot password? </a><div class="clearfix"></div></li>
                        <li>
                            @if (ViewBag.IsCpatchaEmployee == true)
                            {
                                
                                 <div class="g-recaptcha" data-sitekey="@System.Configuration.ConfigurationManager.AppSettings["recaptchaPublicKey"]"></div>

                                <input type="button" value="Login" class="btn signin" id="btnUserLogin" />

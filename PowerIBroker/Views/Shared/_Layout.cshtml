    </div>

    <!--script-->
    <div class="backbg2"></div>   
    <div class="forgPopup" id="forgotPassPopup" role="dialog">     
                <div class="closeCon">
                    <button type="button" class="close" data-dismiss="modal">X</button>
                    <img src="/content/frontend/images/logo.png" class="loginLogo">
                </div>
                <div class="modal-header">
                    <div class="form-group has-feedback feedbackOuter">
                        <input type="text" class="inputTxt1" placeholder="Enter Your Email or Phone" autofocus="" id="forgotPassEmail">
                        <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
                    </div>
                    <button type="submit" class="btn signin" id="btnFrgtPass">Reset Password</button>
                </div>        
    </div>

    <script src="~/Content/Frontend/js/jquery.scrolly.js"></script>
    <script src="~/Content/Frontend/js/jquery.mask.js"></script>

    <script type="text/javascript" src="@Url.Content("~/content/frontend/js/function.js")"></script>

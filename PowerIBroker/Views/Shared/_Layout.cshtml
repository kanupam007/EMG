<script>
    // Validate Captcha
    function onLoginSubmit(token) {
        $.ajax({
            type: "POST",
            url: "/UserLogin/ValidateCaptcha",
            data: "{response: '" + token + "'}",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (captchaResponse) {
                if (captchaResponse.Success) {
                    $("#Loginformsubmit").submit();
                } else {
                }
            }
        });
    }
</script>